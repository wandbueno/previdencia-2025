{
  "name": "@prova-vida/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "ts-node src/scripts/migrate.ts",
    "migrate:prod": "cross-env NODE_ENV=production ts-node src/scripts/migrate.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "update-db": "ts-node src/scripts/update-app-users.ts",
    "update-status": "ts-node src/scripts/migrations/add_submitted_status.ts",
    "update-events": "ts-node src/scripts/update-events-datetime.ts",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": "18.x"
  }
}